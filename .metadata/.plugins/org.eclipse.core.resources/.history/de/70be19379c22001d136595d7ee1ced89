#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout,NULL);
	int e,i;
	printf("Enter a Even number");
	scanf("%d",&e);
	for(i=2;i<=e;i++){
		if(i%2==0){
			printf("%d",i);
		}
	}

	return EXIT_SUCCESS;
}
