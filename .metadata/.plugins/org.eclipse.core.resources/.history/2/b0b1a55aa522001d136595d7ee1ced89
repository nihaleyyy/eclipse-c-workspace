#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout,NULL);
	int n=3,i;
	for(i=0;i<10;i++){
		printf("HI");
		if(i==5){
			continue;
		}
		printf("HELLO");
	}
	printf("FINISHED");

	return EXIT_SUCCESS;
}
