#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout,NULL);
	int a[5],i;
	printf("Enter values");
	for(i=0;i<5;i++){
		scanf("%d",&a[i]);
	}
	printf("Entered a values are %d",a[0]);

	return EXIT_SUCCESS;
}
