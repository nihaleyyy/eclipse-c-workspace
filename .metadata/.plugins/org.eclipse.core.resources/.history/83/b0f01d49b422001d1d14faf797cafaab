#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout,NULL);
	int n,i;
	printf("Enter the Number");
	scanf("%d",&n);
	for(i=1;i<=10;i++){
		printf("%d * %d = %d \n",n,i,n*i);
	}
	return EXIT_SUCCESS;
}
