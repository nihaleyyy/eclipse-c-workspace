#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout,NULL);
	int i=5;
	for(i=0;i<10;i++){
		printf("HI \n");
		if(i==5){
			continue;
		}
		printf("HELLO \n");
	}
	printf("FINISHED \n");

	return EXIT_SUCCESS;
}
