#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout,NULL);
		float mark;
		printf("Enter your total mark percentage");
		scanf("%f",&mark);
		if(mark<0 || mark>100){
			printf("wrong entry");
		}else if(mark<50){
			printf("failed");
		}else if(mark>=50 && mark<60){
			printf("grade E");
		}else if(mark>=60 && mark<70){
			printf("grade D");
		}else if(mark>=70 && mark<80){
			printf("grade C");
		}else if(mark>=80 && mark<90){
			printf("grade B");
		}else{
			printf("grade A");

		}
	return EXIT_SUCCESS;
}
